{
  "name": "kysely-pg-client",
  "version": "0.4.13",
  "author": "Joseph T. Lapp <arachnojoe@gmail.com>",
  "license": "MIT",
  "description": "Non-pooling single-connection Postgres dialect for Kysely",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtlapp/kysely-pg-client"
  },
  "keywords": [
    "kysely",
    "postgres",
    "postgresql",
    "pg",
    "client",
    "dialect",
    "serverless"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "/index.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "rm -rdf dist && tsc && tsc -p tsconfig.cjs.json",
    "test": "jest"
  },
  "dependencies": {
    "kysely": "^0.24.2"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/pg": "^8.6.6",
    "jest": "^29.4.3",
    "pg": "^8.9.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
